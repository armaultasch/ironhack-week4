<!-- _form.html.erb -->
<% if @my_entries.errors.any? %>
<ul>
	<% @my_entries.errors.full_messages.each do |error_msg| %>
	<li> <%= error_msg %> </li>
	<% end %>
</ul>
<% end %>
<%= form_for [@my_project, @my_entries] do |f| %>
	<%= f.label :hours %>
	<%= f.text_field :hours %>
	<br>
	<%= f.label :minutes %>
	<%= f.text_field :minutes %>
	<br>
	<%= f.label :comments %>
	<%= f.text_area :comments %>
	<br>
	<%= f.label :date %>
	<%= f.date_field :date %>
	<br>


	<%= f.button %>
	<% end %>